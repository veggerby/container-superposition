# Environment Variables
#
# Copy this file to .env in your project root to customize
# docker-compose and other service configurations.
#
# Generated by container-superposition init tool

# Prometheus version
PROMETHEUS_VERSION=latest

# Prometheus port
PROMETHEUS_PORT=9090

# Loki version
LOKI_VERSION=latest

# Loki port
LOKI_PORT=3100

# OpenTelemetry Collector version
OTEL_COLLECTOR_VERSION=latest

# OpenTelemetry Collector ports
OTEL_GRPC_PORT=4317
OTEL_HTTP_PORT=4318
OTEL_METRICS_PORT=8888
OTEL_EXPORTER_PORT=8889
OTEL_HEALTH_PORT=13133

# Grafana version
GRAFANA_VERSION=latest

# Grafana admin credentials
GRAFANA_ADMIN_USER=admin
GRAFANA_ADMIN_PASSWORD=admin

# Grafana port
GRAFANA_PORT=3000

# PostgreSQL Configuration
POSTGRES_VERSION=16
POSTGRES_DB=devdb
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_PORT=5432

# Redis Configuration
REDIS_VERSION=7
REDIS_PORT=6379
# REDIS_PASSWORD=your-secure-password
