{
  "portOffset": 0,
  "ports": [
    {
      "port": 16686,
      "service": "jaeger",
      "protocol": "http",
      "description": "Jaeger UI",
      "path": "/",
      "onAutoForward": "openBrowser",
      "actualPort": 16686
    },
    {
      "port": 14250,
      "service": "jaeger",
      "protocol": "grpc",
      "description": "Jaeger gRPC receiver",
      "onAutoForward": "ignore",
      "actualPort": 14250
    },
    {
      "port": 14268,
      "service": "jaeger",
      "protocol": "http",
      "description": "Jaeger HTTP receiver",
      "onAutoForward": "ignore",
      "actualPort": 14268
    },
    {
      "port": 9090,
      "service": "prometheus",
      "protocol": "http",
      "description": "Prometheus web UI and API",
      "path": "/",
      "onAutoForward": "openBrowser",
      "actualPort": 9090
    },
    {
      "port": 4317,
      "actualPort": 4317,
      "service": "otel-collector"
    },
    {
      "port": 4318,
      "actualPort": 4318,
      "service": "otel-collector"
    },
    {
      "port": 8888,
      "actualPort": 8888,
      "service": "otel-collector"
    },
    {
      "port": 8889,
      "actualPort": 8889,
      "service": "otel-collector"
    },
    {
      "port": 5432,
      "service": "postgres",
      "protocol": "tcp",
      "description": "PostgreSQL database connection",
      "onAutoForward": "notify",
      "connectionStringTemplate": "postgresql://{postgres_user}:{postgres_password}@{host}:{port}/{postgres_db}",
      "actualPort": 5432
    }
  ],
  "connectionStrings": {
    "jaeger": "http://localhost:16686/",
    "jaeger-url": "http://localhost:14268",
    "jaeger-14268": "http://localhost:14268",
    "prometheus": "http://localhost:9090/",
    "prometheus-url": "http://localhost:9090/",
    "postgres": "postgresql://postgres:postgres@localhost:5432/devdb"
  }
}
